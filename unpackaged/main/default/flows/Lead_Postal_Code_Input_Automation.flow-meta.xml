    <description>This PB is triggered when the Country = United States and the Zip/Postal Code is input or changed.</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>and(
$User.Override_Workflow__c = false,

isnew(),

[Lead].Country = &quot;United States&quot;,

not(isblank([Lead].PostalCode)),

isblank([Lead].Application_Lookup__c)
)

||

and(
$User.Override_Workflow__c = false,

or(
ischanged([Lead].Country),
ischanged([Lead].PostalCode)
),
 
[Lead].Country = &quot;United States&quot;,

not(isblank([Lead].PostalCode)),

not(ischanged([Lead].Application_Lookup__c))
)


</stringValue>
            </value>
