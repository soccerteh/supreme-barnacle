{!myVariable_current.Country} = &quot;United States&quot;,

not(isblank({!myVariable_current.PostalCode})),

isblank({!myVariable_current.Associated_Application__c})
)

||

and(
{!$User.Override_Workflow__c} = false,

or(
ischanged({!myVariable_current.Country}),
ischanged({!myVariable_current.PostalCode})
),
 
