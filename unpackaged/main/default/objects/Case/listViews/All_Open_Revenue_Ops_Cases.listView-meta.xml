<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>All_Open_Revenue_Ops_Cases</fullName>
    <columns>CASES.CASE_NUMBER</columns>
    <columns>CREATEDBY_USER.ALIAS</columns>
    <columns>CASES.SUBJECT</columns>
    <columns>OWNER_NAME</columns>
    <columns>CASES.STATUS</columns>
    <columns>CASES.PRIORITY</columns>
    <columns>Due_Date__c</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>CASES.TYPE</field>
        <operation>equals</operation>
        <value>Feature Request,Revenue Ops Bug,Revenue Ops Question,Upcoming Sprint Request,New Hire Request</value>
    </filters>
    <filters>
        <field>CASES.STATUS</field>
        <operation>equals</operation>
        <value>In Progress,Needs Grooming,Groomed,On Hold,Escalated,New</value>
    </filters>
    <filters>
        <field>OWNER_NAME</field>
        <operation>contains</operation>
        <value>Revenue,Joe,Aline,Akilah</value>
    </filters>
    <label>All Open Revenue Ops Cases</label>
    <sharedTo>
        <role>Revenue_Operations</role>
    </sharedTo>
</ListView>
