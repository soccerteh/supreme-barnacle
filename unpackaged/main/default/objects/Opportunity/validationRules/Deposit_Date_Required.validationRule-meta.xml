<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Deposit_Date_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(StageName, &#39;Deposit Received&#39;), ISBLANK(Deposit_Date__c) )</errorConditionFormula>
    <errorMessage>You cannot move to Deposit Received until the student has submitted their full deposit.</errorMessage>
</ValidationRule>
