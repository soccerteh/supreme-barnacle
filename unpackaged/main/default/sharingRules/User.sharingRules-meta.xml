<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingGuestRules>
        <fullName>Request_Information_Sharing</fullName>
        <accessLevel>Read</accessLevel>
        <label>Request Information Sharing</label>
        <sharedTo>
            <guestUser>Request_Information</guestUser>
        </sharedTo>
        <criteriaItems>
            <field>IsActive</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <includeHVUOwnedRecords>false</includeHVUOwnedRecords>
    </sharingGuestRules>
    <sharingOwnerRules>
        <fullName>Partner_Sharing</fullName>
        <accessLevel>Read</accessLevel>
        <label>Partner Sharing</label>
        <sharedTo>
            <allPartnerUsers></allPartnerUsers>
        </sharedTo>
        <sharedFrom>
            <group>Internal_Staff_For_Partner_Site</group>
        </sharedFrom>
    </sharingOwnerRules>
</SharingRules>
