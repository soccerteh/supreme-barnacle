<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <outboundMessages>
        <fullName>Zapier_View_Access_to_Box</fullName>
        <apiVersion>50.0</apiVersion>
        <endpointUrl>https://hooks.zapier.com/hooks/catch/9928313/oe1lyh5/</endpointUrl>
        <fields>AccountId</fields>
        <fields>Email</fields>
        <fields>Id</fields>
        <includeSessionId>false</includeSessionId>
        <integrationUser>zapier@vertoeducation.org</integrationUser>
        <name>Zapier: View Access to Box</name>
        <protected>false</protected>
        <useDeadLetterQueue>false</useDeadLetterQueue>
    </outboundMessages>
    <rules>
        <fullName>New Contact to Share on Box</fullName>
        <actions>
            <name>Zapier_View_Access_to_Box</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <formula>AND(
 $User.Override_Workflow__c = FALSE, 
LEFT( RecordTypeId , 15) = LEFT( $Label.RT_Contact_University, 15) , 
Box_View_Access__c = TRUE,
OR(
ISNEW(),
ISCHANGED (Box_View_Access__c)
 ))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
