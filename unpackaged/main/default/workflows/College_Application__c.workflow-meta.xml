    <rules>
        <fullName>Application Submitted Date</fullName>
        <actions>
            <name>Verto_Submit_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>College_Application__c.Application_Status__c</field>
            <operation>equals</operation>
            <value>Sent to University</value>
        </criteriaItems>
        <criteriaItems>
            <field>User.Override_Workflow__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Populate College Application Name</fullName>
        <actions>
            <name>Create_College_Application_Name</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(
 $User.Override_Workflow__c = False, 
OR(
 Name = &quot;X&quot;, 
ISNEW(),
ISCHANGED( University__c  ),
ISCHANGED( Student_Account__c   )))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Submit College Application to University</fullName>
        <actions>
