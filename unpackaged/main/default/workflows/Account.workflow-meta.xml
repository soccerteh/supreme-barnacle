<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <outboundMessages>
        <fullName>Zapier_Create_New_Box_Acount</fullName>
        <apiVersion>50.0</apiVersion>
        <endpointUrl>https://hooks.zapier.com/hooks/catch/9928313/oebf2h9/</endpointUrl>
        <fields>Id</fields>
        <fields>Name</fields>
        <includeSessionId>false</includeSessionId>
        <integrationUser>zapier@vertoeducation.org</integrationUser>
        <name>Zapier: Create New Box Acount</name>
        <protected>false</protected>
        <useDeadLetterQueue>false</useDeadLetterQueue>
    </outboundMessages>
    <rules>
        <fullName>New University Account</fullName>
        <actions>
            <name>Zapier_Create_New_Box_Acount</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>AND(
 $User.Override_Workflow__c = False, 
LEFT( RecordTypeId , 15) = LEFT(  $Label.RT_Account_Partner ,15),
OR(
ISNEW(), 
ISBLANK( Box_Id__c )))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>legacy_Box</fullName>
        <actions>
            <name>Zapier_Create_New_Box_Acount</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <formula>Legacy_box_backfill__c = TRUE</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
